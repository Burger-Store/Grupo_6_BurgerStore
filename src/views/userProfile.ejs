<!DOCTYPE html>
<html lang="es">
<%- include('partials/head'); %>
<body>
	<%- include('partials/header', {user: user}); %>

		<div class="container" style="margin-top: 40px;">
		<div class="row">
			<h1 class="main-title">Usuario</h1>
			<div class="col-md-3">
				<% if (user && user.image) { %>
					<img src="/img/users/<%= user.image %>" alt="img">
				<% } %><img src="/img/users/<%= users[i].image %>" alt="img">
				<div><%= users[i].name + ' ' + users[i].surname %></div>    
				<div><%= users[i].email %></div>
				<div><%= users[i].country %></div>
				<div><%= users[i].address %></div>
				<div><%= users[i].lift %></div>
				<div><%= users[i].city %></div>
				<div><%= users[i].state %></div>
				<div><%= users[i].zipcode %></div>
			</div>
		</div>
	</div>
	
	<%- include('partials/footer'); %>
</body>
</html>

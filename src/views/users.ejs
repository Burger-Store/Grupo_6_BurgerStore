    <%- include('./partials/head') %>
    <title>Users List</title>
    <%- include('./partials/header') %>
    <%- include('./partials/productCart') %>
    <main>
        <h1 class="main-title">Usuarios registrados</h1>
        <div class="section-main">
            <% for(let i=0; i < users.length; i++){%>
            <div class="product">
                <div class="product-description">
                    <img src="/img/users/<%= users[i].image %>" alt="img">
                    <div class="price">Nombre: <%= users[i].name + ' ' + users[i].surname %></div>    
                    <div class="price">Email: <%= users[i].email %></div>
                    <div class="price">Telefono: <%= users[i].phone %></div>
                    <div class="price">Direcci√≥n: <%= users[i].address %></div>
                    <div class="price">Departamento:<%= users[i].lift %></div>
                    <div class="price">Ciudad: <%= users[i].city %></div>
                    <div class="price">Nacionalidad: <%= users[i].country %></div>
                    <div class="price">Codigo Postal: <%= users[i].zipcode %></div>
                    <div class="price">privilegio tipo: <%= users[i].idusertype %></div>
                    <div class="add-button">
                        <form action="/user/update/<%= users[i].idusers %>" method="get">
                            <button class="button blue descripcion" type="submit">Editar</button>
                        </form>
                    </div>
                    <div class="add-button">
                        <form action="/user/<%= users[i].idusers %>/delete?_method=DELETE" method="POST">
                            <button class="button red descripcion" type="submit" onclick="return confirm( 'Desea eliminar este usuario?');">Eliminar</button>
                        </form>
                    </div>
                </div>
            </div>
            <%}%> 
        </div>
    </main>
    <%- include('./partials/footer') %>